{
  "name": "@peac/pref",
  "version": "0.9.12.1",
  "description": "AIPREF resolver with robots.txt bridge",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
