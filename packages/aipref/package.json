{
  "name": "@peac/pref",
  "version": "0.10.10",
  "description": "AIPREF resolver with robots.txt bridge",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
