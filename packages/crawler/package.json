{
  "name": "@peac/crawler",
  "version": "0.9.12.1",
  "description": "PEAC Protocol crawler control with vendor-neutral providers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "dev"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest --config jest.config.ts --coverage",
    "test:ci": "jest --config jest.config.ts --coverage --ci --runInBand --forceExit",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "test:integration": "jest --config jest.config.ts --testPathPattern=integration",
    "test:unit": "jest --config jest.config.ts --testPathPattern=unit",
    "test:smoke": "jest --config jest.config.ts --selectProjects . --testPathPattern='(unit|smoke)'",
    "test:watch": "jest --config jest.config.ts --watch",
    "lint": "echo 'Lint temporarily disabled due to workspace dependencies - run from root'",
    "typecheck": "echo 'Crawler typecheck temporarily disabled - has TypeScript compatibility issues'",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "perf": "node --loader ts-node/esm test/performance/perf.ts"
  },
  "dependencies": {
    "lru-cache": "^10.0.0",
    "@peac/core": "workspace:^0.9.12.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "tsup": "^8.0.0"
  },
  "keywords": [
    "peac",
    "crawler",
    "verification",
    "cloudflare",
    "circuit-breaker"
  ],
  "author": "PEAC Protocol",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
