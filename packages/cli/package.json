{
  "name": "@peac/cli",
  "version": "0.10.9",
  "description": "PEAC protocol command-line tools",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": {
    "peac": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peacprotocol/peac.git",
    "directory": "packages/cli"
  },
  "author": "jithinraj <7850727+jithinraj@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/peacprotocol/peac/issues"
  },
  "homepage": "https://github.com/peacprotocol/peac#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:types",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "build:js": "tsup",
    "build:types": "rm -f dist/.tsbuildinfo && tsc"
  },
  "dependencies": {
    "@peac/audit": "workspace:*",
    "@peac/crypto": "workspace:*",
    "@peac/disc": "workspace:*",
    "@peac/policy-kit": "workspace:*",
    "@peac/protocol": "workspace:*",
    "@peac/schema": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "tsup": "^8.0.0"
  }
}
