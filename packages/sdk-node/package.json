{
  "name": "@peacprotocol/sdk",
  "version": "0.9.11",
  "description": "Official Node.js SDK for PEAC Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./policy": {
      "import": "./dist/policy.js",
      "require": "./dist/policy.cjs",
      "types": "./dist/policy.d.ts"
    },
    "./headers": {
      "import": "./dist/headers.js",
      "require": "./dist/headers.cjs",
      "types": "./dist/headers.d.ts"
    },
    "./receipts": {
      "import": "./dist/receipts.js",
      "require": "./dist/receipts.cjs",
      "types": "./dist/receipts.d.ts"
    },
    "./ratelimit": {
      "import": "./dist/ratelimit.js",
      "require": "./dist/ratelimit.cjs",
      "types": "./dist/ratelimit.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.cjs",
      "types": "./dist/adapters/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18"
  },
  "sideEffects": false,
  "repository": "github:peacprotocol/peac",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "test": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "peac",
    "protocol",
    "sdk",
    "web-bot-auth",
    "receipts",
    "policy",
    "rate-limiting"
  ],
  "author": "PEAC Protocol Foundation",
  "license": "Apache-2.0",
  "dependencies": {
    "@noble/ed25519": "^2.1.0",
    "@peacprotocol/schema": "file:../schema",
    "undici": "^6.6.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.1",
    "fast-check": "^3.15.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "homepage": "https://peacprotocol.org",
  "bugs": {
    "url": "https://github.com/peacprotocol/peac/issues"
  }
}
