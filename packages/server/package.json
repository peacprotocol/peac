{
  "name": "@peacprotocol/server",
  "version": "0.9.9",
  "description": "PEAC Protocol: Programmable Environment for Agent Coordination",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18"
  },
  "sideEffects": false,
  "repository": "github:peacprotocol/peac",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --coverage --forceExit",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "e2e:verify": "node scripts/e2e/verify-success.mjs",
    "e2e:ssrf": "node scripts/e2e/ssrf-check.mjs",
    "verify:release": "bash tools/verify-0.9.3.sh",
    "smoke": "bash tools/smoke.sh",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "test:conformance": "jest test/conformance --coverage",
    "bench": "tsx scripts/microbench.capabilities.ts",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output-format=json --output-file=sbom.json",
    "audit": "npm audit --audit-level=moderate",
    "scan:deps": "npx @sonatype/ossindex-cli@latest audit -f package.json",
    "validate:peip": "tsx scripts/validate-peip.ts",
    "prepublishOnly": "npm run build && npm run test:coverage && npm run sbom"
  },
  "dependencies": {
    "@peacprotocol/schema": "file:../schema",
    "@types/uuid": "^10.0.0",
    "bullmq": "^5.1.0",
    "canonicalize": "^2.0.0",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.5.0",
    "ipaddr.js": "^2.1.0",
    "jose": "^5.10.0",
    "json-stable-stringify": "^1.3.0",
    "pino": "^8.17.0",
    "prom-client": "^15.1.0",
    "tldts": "^7.0.12",
    "ulidx": "^2.4.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.19.0",
    "@redocly/cli": "^2.0.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.1",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "license": "Apache-2.0",
  "homepage": "https://peacprotocol.org",
  "bugs": {
    "url": "https://github.com/peacprotocol/peac/issues"
  },
  "keywords": [
    "peac",
    "payments",
    "idempotency",
    "jwks",
    "rfc7807",
    "ratelimit",
    "capabilities",
    "dpop",
    "w3c-trace-context"
  ]
}
