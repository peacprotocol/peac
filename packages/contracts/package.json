{
  "name": "@peac/contracts",
  "version": "0.10.9",
  "description": "PEAC canonical error codes and verification mode contracts",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:types",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build:js": "tsup",
    "build:types": "rm -f dist/.tsbuildinfo && tsc"
  },
  "keywords": [
    "peac",
    "protocol",
    "errors",
    "rfc9457",
    "http"
  ],
  "author": "PEAC Protocol contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/peacprotocol/peac.git",
    "directory": "packages/contracts"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^2.0.0",
    "tsup": "^8.0.0"
  }
}
