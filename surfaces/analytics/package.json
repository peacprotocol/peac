{
  "name": "@peac/analytics",
  "version": "0.10.11",
  "private": true,
  "description": "Analytics surface for PEAC Protocol - metrics API with k-anonymity protection",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@peac/privacy": "workspace:*",
    "@peac/kernel": "workspace:*",
    "@peac/schema": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  },
  "sideEffects": false
}
